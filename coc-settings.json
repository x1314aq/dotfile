{
    "list.normalMappings": {
        "q":"do:exit"
    },
    "lists.disabledLists": ["tags"],
    "coc.preferences.extensionUpdateCheck": "never",
    "session.saveOnVimLeave": false,
    "list.source.location.defaultOptions": ["--normal"],
    "list.maxPreviewHeight": 5,
    "list.source.grep.defaultOptions": ["--normal"],
    "list.source.grep.useLiteral": false,
    "list.source.sessions.defaultOptions": ["--normal"],
    "suggest.minTriggerInputLength": 2,
    "signature.target": "echo",
    "languageserver": {
        "ccls": {
            "command": "ccls",
            "filetypes": ["c", "c.doxygen", "cpp", "objc"],
            "rootPatterns": [".ccls", "compile_commands.json", ".vim", ".git"],
            "trace.server": "verbose",
            "initializationOptions": {
                "cache": {
                    "directory": "/tmp/ccls-cache"
                },
                "highlight": {"lsRanges": true}
            }
        },
        "cmake": {
            "command": "cmake-language-server",
            "filetypes": ["cmake"],
            "rootPatterns": [
                "build/"
            ],
            "initializationOptions": {
                "buildDirectory": "build"
            }
        }
    }
}
